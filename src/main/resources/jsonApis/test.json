{
   "seq":["postStudent","getStudent","getAllStudent"],
   "baseUri":"http://localhost:4567",
   "apiHandler":"com.rf.apis.handlers.DefaultApiHandler",
   "properties":{
      "api":"/student"
      "p":"{{R_PERSON}}"	  
    },
    
    "di":{
    	"postStudent":[
    		{
    			"paramName":"id",
                "jsonPath":"id"
    		}
    	]
    },
    
	"apis":{
		
		"postStudent":{
		  "basePath":"{{api}}",
		  "method":"POST",
		  "body":{
			   "name":"{{R_FIRST_NAME}} {{R_LAST_NAME}}",
			   "age": "{{N2}}",
			   "grade":"{{N1}}th"
		  },
		  "assertion":{
			  "status":"200"
		  }
	     },
		 
		"getStudent":{
		   "basePath":"{{api}}",
		   "method":"GET",
		   "queryParams":{
		        "id":"{{id}}"
		   }
		 },
		 
		 "getAllStudent":{
			   "basePath":"{{api}}",
			   "method":"GET"
		 }
	}
}	